<div id="<%= dom_id person %>" class="my-3">
  <p>
    <strong>Nome:</strong>
    <%= person.name %>
  </p>

  <p>
    <strong>Telefone:</strong>
    <%= person.phone_number %>
  </p>

  <p>
    <strong>CPF / CNPJ:</strong>
    <%= person.national_id %>
  </p>

  <p>
    <strong>Ativo:</strong>
    <%= person_active(person) %>
  </p>

  <p>
    <strong>Total da Conta</strong>
    <%= person.balance %>
  </p>

</div>
